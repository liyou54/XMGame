{
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.gitignore": true,
        "**/.gitmodules": true,
        "**/*.booproj": true,
        "**/*.pidb": true,
        "**/*.suo": true,
        "**/*.user": true,
        "**/*.userprefs": true,
        "**/*.unityproj": true,
        "**/*.dll": true,
        "**/*.exe": true,
        "**/Library": true,
        "**/Temp": true,
        "**/Obj": true,
        "**/Build": true,
        "**/Builds": true,
        "**/.vs": true,
        "**/.vscode": false,
        "**/*.meta": true
    },
    "omnisharp.useModernNet": true,
    "omnisharp.enableRoslynAnalyzers": true,
    "omnisharp.enableEditorConfigSupport": true,
    "omnisharp.enableAsyncCompletion": true,
    "omnisharp.projectLoadTimeout": 120,
    "omnisharp.maxProjectResults": 250,
    "files.associations": {
        "*.asmdef": "json",
        "*.asmref": "json",
        "*.meta": "yaml"
    },
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": "explicit"
    },
    "files.encoding": "utf8",
    "files.eol": "\n",
    "locale": "zh-cn",
    "search.exclude": {
        "**/Library": true,
        "**/Temp": true,
        "**/Obj": true,
        "**/Build": true,
        "**/Builds": true,
        "**/*.csproj": false,
        "**/*.sln": false
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/Library/**": true,
        "**/Temp/**": true,
        "**/Obj/**": true,
        "**/Build/**": true,
        "**/Builds/**": true
    },
    "dotnet.defaultSolution": "XMGame.sln",
    "dotnet.completion.showCompletionItemsFromUnimportedNamespaces": true,
    "dotnet.formatting.organizeImportsOnFormat": true
}